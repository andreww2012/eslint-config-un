    "@types/node": "20.17.46",
    "@types/semver": "7.7.0",
    "angular-eslint-plugin-template17": "npm:@angular-eslint/eslint-plugin-template@17.5.3",
    "angular-eslint-plugin18": "npm:@angular-eslint/eslint-plugin@18.4.3",
    "cross-env": "7.0.3",
    "cspell": "9.1.1",
    "diff": "8.0.2",
    "eslint": "9.29.0",
    "eslint-plugin-better-tailwindcss": "3.2.0",
    "eslint-plugin-no-type-assertion": "1.3.0",
    "eslint-plugin-prettier": "5.4.1",
    "eslint-plugin-storybook": "9.0.10",
    "eslint-plugin-tailwindcss": "3.18.0",
    "eslint-plugin-turbo": "2.5.4",
    "eslint-typegen": "2.2.0",
    "npm-check-updates": "18.0.1",
    "prettier": "3.5.3",
    "release-it": "19.0.3",
    "shiki": "3.6.0",
    "shx": "0.4.0",
    "tsup": "8.5.0",
    "tsx": "4.20.3",
    "typescript": "5.8.3"
  },
  "peerDependencies": {
    "@graphql-eslint/eslint-plugin": "^4.4.0",
    "eslint": "^9.15.0",
    "eslint-plugin-better-tailwindcss": "^3.2.0",
    "eslint-plugin-storybook": "^9.0.9",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "eslint-plugin-turbo": "^2.5.4"
  },
  "peerDependenciesMeta": {
    "@graphql-eslint/eslint-plugin": {
      "optional": true
    },
    "eslint-plugin-better-tailwindcss": {
      "optional": true
    },
    "eslint-plugin-storybook": {
      "optional": true
    },
    "eslint-plugin-tailwindcss": {
      "optional": true
    },
    "eslint-plugin-turbo": {
      "optional": true
    }
  },
  "bundleDependencies": [
    "eslint-plugin-prettier",
    "angular-eslint-plugin-template17",
    "angular-eslint-plugin18",
    "eslint-plugin-no-type-assertion"
  ],
  "packageManager": "pnpm@10.12.1",
  "engines": {
    "node": "^20.11 || >=21.1.0"
  },
  "overrides": {
    "eslint-plugin-jest": {
      "@typescript-eslint/eslint-plugin": "$@typescript-eslint/eslint-plugin"
    },
    "eslint-plugin-unused-imports": {
      "@typescript-eslint/eslint-plugin": "$@typescript-eslint/eslint-plugin"
    },
    "eslint-processor-vue-blocks": {
      "@vue/compiler-sfc": "$@vue/compiler-sfc"
    }
  }
}
