{
  "name": "eslint-config-un",
  "version": "0.10.0",
  "description": "A universal-ish ESLint config aiming to be reasonably strict and easily configurable.",
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "homepage": "https://github.com/andreww2012/eslint-plugin-un",
  "bugs": {
    "url": "https://github.com/andreww2012/eslint-plugin-un/issues/new"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andreww2012/eslint-plugin-un.git"
  },
  "license": "MIT",
  "author": "andreww2012",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "u": "nr ua -t minor",
    "ua": "ncu -i && nr typegen",
    "spellcheck": "cspell --no-progress --no-summary .",
    "format:check": "prettier --check '**/*.{js,jsx,cjs,mjs,ts,tsx,cts,mts,vue}' --log-level warn",
    "format:fix": "prettier --write '**/*.{js,jsx,cjs,mjs,ts,tsx,cts,mts,vue}' --log-level warn",
    "eslint:inspect": "npx @eslint/config-inspector --open false",
    "lint:eslint": "eslint --cache",
    "lint:eslint:fix": "nr lint:eslint --fix",
    "lint:types": "tsc --noEmit",
    "lint": "nr lint:types && nr format:check && nr lint:eslint",
    "lint:fix": "nr lint:eslint:fix && nr format:fix",
    "typegen": "tsx scripts/typegen.ts",
    "build:code": "tsup --clean",
    "build:code:test": "tsup --silent --out-dir temp",
    "build": "nr typegen && nr build:code",
    "test": "nr typegen && nr spellcheck && nr lint && nr build:code:test",
    "lint:public-types": "npx @arethetypeswrong/cli --pack .",
    "prepublishOnly": "nr test && nr build && nr lint:public-types"
  },
  "dependencies": {
    "@angular-eslint/eslint-plugin": "20.0.0",
    "@angular-eslint/eslint-plugin-template": "20.0.0",
    "@angular-eslint/template-parser": "20.0.0",
    "@antfu/utils": "9.2.0",
    "@eslint-community/eslint-plugin-eslint-comments": "4.5.0",
    "@eslint-react/eslint-plugin": "1.52.2",
    "@eslint/compat": "1.3.0",
    "@eslint/css": "0.9.0",
    "@eslint/markdown": "6.5.0",
    "@html-eslint/eslint-plugin": "0.41.0",
    "@html-eslint/parser": "0.41.0",
    "@next/eslint-plugin-next": "15.3.3",
    "@stylistic/eslint-plugin": "4.4.1",
    "@tanstack/eslint-plugin-query": "5.78.0",
    "@typescript-eslint/eslint-plugin": "8.34.0",
    "@typescript-eslint/parser": "8.34.0",
    "@vitest/eslint-plugin": "1.2.5",
    "@vue/compiler-sfc": "3.5.16",
    "astro-eslint-parser": "1.2.2",
    "consola": "3.4.2",
    "defu": "6.1.4",
    "ember-eslint-parser": "0.5.9",
    "enhanced-resolve": "5.18.1",
    "es-toolkit": "1.39.3",
    "eslint-config-flat-gitignore": "2.1.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-import-resolver-typescript": "4.4.3",
    "eslint-merge-processors": "2.0.0",
    "eslint-plugin-astro": "1.3.1",
    "eslint-plugin-ava": "15.0.1",
    "eslint-plugin-case-police": "2.0.0",
    "eslint-plugin-css": "0.11.0",
    "eslint-plugin-cypress": "5.1.0",
    "eslint-plugin-de-morgan": "1.3.0",
    "eslint-plugin-depend": "1.2.0",
    "eslint-plugin-ember": "12.5.0",
    "eslint-plugin-erasable-syntax-only": "0.3.1",
    "eslint-plugin-es-x": "8.7.0",
    "eslint-plugin-html": "8.1.3",
    "eslint-plugin-import-x": "4.15.2",
    "eslint-plugin-jest": "28.13.5",
    "eslint-plugin-jest-extended": "3.0.0",
    "eslint-plugin-jsdoc": "51.0.1",
    "eslint-plugin-json-schema-validator": "5.4.1",
    "eslint-plugin-jsonc": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-math": "0.10.1",
    "eslint-plugin-n": "17.20.0",
    "eslint-plugin-no-unsanitized": "4.1.2",
    "eslint-plugin-node-dependencies": "1.1.2",
    "eslint-plugin-package-json": "0.39.2",
    "eslint-plugin-perfectionist": "4.14.0",
    "eslint-plugin-pinia": "0.4.1",
    "eslint-plugin-pnpm": "0.3.1",
    "eslint-plugin-prefer-arrow-functions": "3.6.2",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-qwik": "1.14.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-compiler": "19.1.0-rc.2",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "eslint-plugin-regexp": "2.9.0",
    "eslint-plugin-security": "3.0.1",
    "eslint-plugin-solid": "0.14.5",
    "eslint-plugin-sonarjs": "3.0.2",
    "eslint-plugin-svelte": "3.9.2",
    "eslint-plugin-testing-library": "7.5.3",
    "eslint-plugin-toml": "0.12.0",
    "eslint-plugin-unicorn": "59.0.1",
    "eslint-plugin-unused-imports": "4.1.4",
    "eslint-plugin-vue": "10.2.0",
    "eslint-plugin-vuejs-accessibility": "2.4.1",
    "eslint-plugin-yml": "1.18.0",
    "eslint-processor-vue-blocks": "2.0.0",
    "eslint-rule-composer": "0.3.0",
    "globals": "16.2.0",
    "is-in-editor": "0.2.0",
    "jsonc-eslint-parser": "2.4.0",
    "klona": "2.0.6",
    "local-pkg": "1.1.1",
    "package-manager-detector": "1.3.0",
    "read-package-up": "11.0.0",
    "semver": "7.7.2",
    "svelte-eslint-parser": "1.2.0",
    "toml-eslint-parser": "0.10.0",
    "type-fest": "4.41.0",
    "typescript-eslint": "8.34.0",
    "vue-eslint-parser": "10.1.3",
    "yaml-eslint-parser": "1.3.0"
  },
  "devDependencies": {
    "@antfu/ni": "25.0.0",
    "@arethetypeswrong/cli": "0.18.2",
    "@eslint/config-inspector": "1.1.0",
    "@graphql-eslint/eslint-plugin": "4.4.0",
    "@jest/environment": "30.0.0",
    "@jest/expect": "30.0.0",
    "@sveltejs/kit": "2.21.5",
